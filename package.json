{
  "name": "mernauth",
  "version": "0.0.0",
  "private": true,
  "proxy": "http://localhost:3001",
  "scripts": {
    "start": "node ./bin/www",
    "dev": "concurrently \"npm run startserver\" \"npm run startclient\"",
    "startserver": "nodemon ./bin/www",
    "startclient": "npm start --prefix client",
    "install": "cd client && npm install"
  },
  "dependencies": {
    "concurrently": "^5.2.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1"
  }
}
